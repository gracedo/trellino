<div class="row">
  <div class="col-md-4 col-md-offset-2">
    <h2>My Boards</h2>

    <ul>
      <% boards.each(function(board) { %>
        <li><a href="#/boards/<%= board.escape("id") %>"><%= board.escape('title') %></a></li>
      <% }) %>
    </ul>

    <br>

    <a href="#/boards/new">New Board</a>
  </div>
</div>